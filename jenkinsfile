pipeline {
    agent any

    stages {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'MAVEN') {
                 
                echo "JJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJ"
                      bat ' mvn -f SpringBootRestApiExample/pom.xml clean package' 
                   script{   
                 final changeSet = build.getChangeSet()
final changeSetIterator = changeSet.iterator()
while (changeSetIterator.hasNext()) {
  final gitChangeSet = changeSetIterator.next()
  for (final path : gitChangeSet.getPaths()) {
                  echo "JJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJ"

    println path.getPath()
    echo path.getPath()
    
                    echo "JJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJJ"

  }
}
                }
            }
        }

}
        stage ('Deployment Stage') {
            steps {
                withMaven(maven : 'MAVEN') {
                    bat 'mvn -f SpringBootRestApiExample/pom.xml install'
                   bat 'java -jar SpringBootRestApiExample/target/SpringBootRestApiExample-1.0.0.jar'
                }
            }
        }
    }
}



