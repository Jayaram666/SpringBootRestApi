pipeline {
    agent any

    stages {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'MAVEN') {
                
                      bat ' mvn -f SpringBootRestApiExample/pom.xml clean compile' 
                }
            }
        }


        stage ('Deployment Stage') {
            steps {
                withMaven(maven : 'MAVEN') {
                    bat 'mvn SpringBootRestApiExample/target/SpringBootRestApiExample.jar install'
                }
            }
        }
    }
}
