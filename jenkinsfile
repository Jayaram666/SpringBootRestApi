pipeline {
    agent any

    stages {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'MAVEN') {
      
                sh ' mvn -f SpringBootRestApiExample/pom.xml clean package' 
          
        }

}

        stage ('Deployment Stage') {
            steps {
                withMaven(maven : 'MAVEN') {
                    sh 'mvn -f SpringBootRestApiExample/pom.xml install'
                   sh 'java -jar SpringBootRestApiExample/target/SpringBootRestApiExample-1.0.0.jar'
                }
            }
        }
    }
}





