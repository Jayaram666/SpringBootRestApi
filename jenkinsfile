pipeline {
    agent any

    stages {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'MAVEN') {
                
                      bat ' mvn -f SpringBootRestApiExample/pom.xml clean package' 
                }
            }
        }


        stage ('Deployment Stage') {
            steps {
                withMaven(maven : 'MAVEN') {
                    bat 'mvn -f SpringBootRestApiExample/pom.xml install'
                   bat 'java -jar com/websystique/springboot/SpringBootRestApiExample/1.0.0/SpringBootRestApiExample-1.0.0.jar'
                }
            }
        }
    }
}
